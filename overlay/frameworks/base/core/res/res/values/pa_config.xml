<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Hardware keys present on the device, stored as a bit field.
         This integer should equal the sum of the corresponding value for each
         of the following keys present:
             1 - Home
             2 - Back
             4 - Menu
             8 - Assistant (search)
            16 - App switch
            32 - Camera
         For example, a device with Home, Back and Menu keys would set this
         config to 7. -->
    <integer name="config_deviceHardwareKeys">19</integer>

    <!-- Whether device has fysical tri state switch -->
    <bool name="config_hasAlertSlider">true</bool>

    <!-- Whether device has dash charging support -->
    <bool name="config_hasDashCharger">true</bool>

    <!-- Extended Boosts for QPerformance -->

    <!-- Boost parameters which get passed to QPerformance -->
    <integer-array name="onbootboost_param_value">
       <item>0x40C00000</item> <!-- SCHEDBOOST resource opcode-->
       <item>0x1</item>        <!-- SCHEDBOOST resource value-->
       <item>0x40804000</item> <!-- CPUBOOST_MAX_FREQ BIG Core resource opcode-->
       <item>0xFFF</item>      <!-- CPUBOOST MAX_FREQ resource value-->
       <item>0x40804100</item> <!-- CPUBOOST_MAX_FREQ LITTLE Core resource opcode-->
       <item>0xFFF</item>      <!-- CPUBOOST MAX_FREQ resource value-->
       <item>0x40800100</item> <!-- CPUBOOST_MIN_FREQ LITTLE Core resource opcode -->
       <item>0x6A4</item>      <!-- CPUBOOST MIN_FREQ resource value -->
       <item>0x40800000</item> <!-- CPUBOOST_MIN_FREQ BIG CORE resource opcode -->
       <item>0x7D0</item>      <!-- CPUBOOST MIN_FREQ resource value -->
    </integer-array>

    <!-- Boost parameters which get passed to QPerformance -->
    <integer-array name="qboost_weak_param_value">
        <item>0x40800100</item> <!-- CPUBOOST_MIN_FREQ LITTLE Core resource opcode -->
        <item>0x514</item>      <!-- CPUBOOST MIN_FREQ resource value -->
        <item>0x40800000</item> <!-- CPUBOOST_MIN_FREQ BIG CORE resource opcode -->
        <item>0x578</item>      <!-- CPUBOOST MIN_FREQ resource value -->
    </integer-array>
    <integer-array name="qboost_strong_param_value">
        <item>0x40804000</item> <!-- CPUBOOST_MAX_FREQ BIG Core resource opcode-->
        <item>0xFFF</item>      <!-- CPUBOOST MAX_FREQ resource value-->
        <item>0x40804100</item> <!-- CPUBOOST_MAX_FREQ LITTLE Core resource opcode-->
        <item>0xFFF</item>      <!-- CPUBOOST MAX_FREQ resource value-->
        <item>0x41800000</item> <!-- CPUBW_MIN_FREQ resource opcode-->
        <item>140</item>        <!-- CPUBW_MIN_FREQ resource value-->
        <item>0x40800100</item> <!-- CPUBOOST_MIN_FREQ LITTLE Core resource opcode -->
        <item>0x578</item>      <!-- CPUBOOST MIN_FREQ resource value -->
        <item>0x40800000</item> <!-- CPUBOOST_MIN_FREQ BIG CORE resource opcode -->
        <item>0x6A4</item>      <!-- CPUBOOST MIN_FREQ resource value -->
        <item>0x40400000</item> <!-- PWR_CLPS DIS resource opcode-->
        <item>0x1</item>        <!-- PWR_CLPS DIS resource value-->
    </integer-array>

    <!-- Keypress Boost duration -->
    <integer name="fpunlockboost_duration">800</integer>

    <!-- sRGB sysfs path -->
    <string name="config_srgb_path">/sys/class/graphics/fb0/SRGB</string>

    <!-- Internal default to navigation bar value -->
    <bool name="config_defaultToNavigationBar">false</bool>

    <!-- Gestures' key codes. If the gesture is not supported set it to 0. -->
    <integer name="config_doubleTapKeyCode">59</integer>
    <integer name="config_drawOKeyCode">60</integer>
    <integer name="config_twoFingerSwipeKeyCode">61</integer>
    <integer name="config_drawVKeyCode">62</integer>
    <integer name="config_drawArrowLeftKeyCode">63</integer>
    <integer name="config_drawArrowRightKeyCode">64</integer>
    <integer name="config_drawInverseVKeyCode">65</integer>
    <integer name="config_oneFingerSwipeLeftKeyCode">66</integer>
    <integer name="config_oneFingerSwipeRightKeyCode">67</integer>
    <integer name="config_drawMKeyCode">68</integer>
    <integer name="config_drawWKeyCode">87</integer>
    <integer name="config_oneFingerSwipeUpKeyCode">88</integer>
    <integer name="config_oneFingerSwipeDownKeyCode">183</integer>

    <!-- Default settings for gestures
             0 - Disabled
             1 - Wake device
             2 - Toggle flashlight
             3 - Toggle airplane mode
             4 - Play/Pause music
             5 - Next music
             6 - Previous music
             7 - Open camera
             8 - Open dialer -->
    <integer name="config_doubleTapDefault">1</integer>
    <integer name="config_drawVDefault">2</integer>
    <integer name="config_drawODefault">7</integer>
    <integer name="config_drawArrowLeftDefault">6</integer>
    <integer name="config_drawArrowRightDefault">5</integer>

    <!-- Whether to intercept inputs on screen off -->
    <bool name="config_enableKeyHandler">true</bool>

    <!-- Whether notify fingerprint client of successful cancelled authentication -->
    <bool name="config_notifyClientOnFingerprintCancelSuccess">true</bool>
</resources>
